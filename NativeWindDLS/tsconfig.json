{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "types": ["jest"],
    "baseUrl": ".",
    "paths": {
      "@*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@dls/*": ["src/dls/*"]
